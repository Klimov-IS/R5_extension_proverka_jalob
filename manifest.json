{
  "manifest_version": 3,
  "name": "Feedback Checker",
  "version": "2.3.2",
  "key": "",
  "description": "Проверяет одобренные фидбеки на Wildberries, делает скриншоты и сохраняет их на Google Drive.",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "identity",
    "storage",
    "webRequest"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Feedback Checker"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>",
    "*://*.wildberries.ru/*",
    "https://www.googleapis.com/*",
    "https://accounts.google.com/*",
    "https://rating5.ru/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.wildberries.ru/*"],
      "js": ["deduplication-cache.js", "content.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["html2canvas.min.js"],
      "matches": ["*://*.wildberries.ru/*"]
    }
  ],
  "chrome_url_overrides": {}
}
